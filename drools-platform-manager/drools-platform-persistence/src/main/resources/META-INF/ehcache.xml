<ehcache>
  <diskStore path="java.io.tmpdir"/>

  <cache name="sampleCache1"
         maxElementsInMemory="10000"
         eternal="false"
         overflowToDisk="true"
         timeToIdleSeconds="300"
         timeToLiveSeconds="600"/>

  <defaultCache
          maxElementsInMemory="10000"
          eternal="false"
          overflowToDisk="true"
          timeToIdleSeconds="300"
          timeToLiveSeconds="600">

  </defaultCache>
</ehcache>